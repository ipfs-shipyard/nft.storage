{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "test": "eslint './**/*.js'"
  },
  "dependencies": {
    "@magic-ext/oauth": "^0.7.0",
    "filesize": "^6.1.0",
    "magic-sdk": "^4.2.1",
    "next": "^10.1.3",
    "nft.storage": "^0.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.13.9",
    "swagger-ui-react": "^3.45.0",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "env": {
      "es2020": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": [
        "error",
        {
          "args": "all",
          "argsIgnorePattern": "_",
          "varsIgnorePattern": "_"
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/display-name": 1
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "node_modules",
    ".next",
    "out"
  ]
}
